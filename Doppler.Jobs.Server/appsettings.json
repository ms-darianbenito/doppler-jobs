{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Hangfire": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jobs": {
    "WorkerServiceJob": {
      "IntervalCronExpression": "0 18 * * mon-fri",
      "Identifier": "Doppler_Sap_Job"
    }
  },
  "TimeZoneJobs": "Argentina Standard Time",
  "HttpClient": {
    "Client": {
      "ClientName": "httpClient",
      "Policies": {
        "RetryAttemps": 3
      }
    }
  },
  "DopplerCurrencyService": {
    "Url": "https://localhost:5001/Currency/",
    "CurrencyCode": [ "Ars" ]
  }
}
